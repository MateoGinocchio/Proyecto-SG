.animacion-mensaje {
  border: 5px #e9a5a5 ridge;
  transition: all 2s;
  width: 25%;
  /*Chrome y Edge*/
  -webkit-transition: all 2s;
  /*Mozilla*/
  -moz-transition: all 2s;
  /*Opera*/
  -o-transition: all 2s;
}
.animacion-mensaje:hover {
  transform: scale(1.5);
  /*Chrome y Edge*/
  -webkit-transform: scale(1.5);
  /*Mozilla*/
  -moz-transform: scale(1.5);
  /*Opera*/
  -o-transform: scale(1.5);
}

